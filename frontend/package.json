{
  "name": "petshop-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html,json}\"",
    "format:watch": "onchange \"src/**/*.{js,jsx,css,html,json}\" -- prettier --write {{changed}}",
    "dev:format": "concurrently \"npm run dev\" \"npm run format:watch\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.3",
    "axios": "^1.8.4",
    "prop-types": "^15.8.1",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.5.1",
    "swiper": "^11.2.6",
    "tailwindcss": "^4.1.3",
    "@reduxjs/toolkit": "^2.7.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "dom": "^0.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "onchange": "^7.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^19.1.0",
    "vite": "^6.2.5"
  }
}
